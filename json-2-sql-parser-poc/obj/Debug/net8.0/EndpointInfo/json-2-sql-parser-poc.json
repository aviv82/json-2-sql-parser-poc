  "components": {
    "schemas": {
      "FieldGroupEnum": {
        "enum": [
          "None",
          "All",
          "Basic",
          "Descriptive",
          "System"
        ],
        "type": "string"
      },
      "IshField": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "level": {
            "$ref": "#/components/schemas/IshFieldLevelEnum"
          }
        },
        "additionalProperties": false
      },
      "IshFieldLevelEnum": {
        "enum": [
          "Language",
          "Logical",
          "Version"
        ],
        "type": "string"
      },
      "ListOfValuesEnum": {
        "enum": [
          "DATCLEGALENTITY",
          "DATCMACHINECATEGORY"
        ],
        "type": "string"
      },
      "PublicationsByLogicalIdRequest": {
        "type": "object",
        "properties": {
          "logicalIds": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "statusFilter": {
            "$ref": "#/components/schemas/StatusFilterEnum"
          },
          "selectedProperties": {
            "$ref": "#/components/schemas/SelectedPropertiesEnum"
          },
          "fieldGroup": {
            "$ref": "#/components/schemas/FieldGroupEnum"
          },
          "fields": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RequestField"
            },
            "nullable": true
          },
          "includeLinks": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "RequestField": {
        "type": "object",
        "properties": {
          "ishField": {
            "$ref": "#/components/schemas/IshField"
          }
        },
        "additionalProperties": false
      },
      "SelectedPropertiesEnum": {
        "enum": [
          "Id",
          "TitleAndDescription"
        ],
        "type": "string"
      },
      "StatusFilterEnum": {
        "enum": [
          "All",
          "AllReleased",
          "DraftOrLatestReleased",
          "LatestReleased"
        ],
        "type": "string"
      }
    }
  }
